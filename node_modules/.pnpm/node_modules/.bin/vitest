#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.1_jsdom@24.1.0_terser@5.31.0/node_modules/vitest/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.1_jsdom@24.1.0_terser@5.31.0/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.1_jsdom@24.1.0_terser@5.31.0/node_modules/vitest/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/vitest@1.6.0_@types+node@20.14.1_jsdom@24.1.0_terser@5.31.0/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
