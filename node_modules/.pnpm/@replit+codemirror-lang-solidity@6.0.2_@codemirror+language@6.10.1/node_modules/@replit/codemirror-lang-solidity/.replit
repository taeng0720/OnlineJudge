run="yarn run dev"

entrypoint = "src/index.ts"

[packager]
language = "nodejs-yarn"
[packager.features]
packageSearch = true
guessImports = true

[languages.js]
pattern = "**/*.ts"
[languages.js.languageServer]
start = ["typescript-language-server", "--stdio"]

[[ports]]
localPort = 443
externalPort = 80

[[ports]]
localPort = 3000
externalPort = 3000
