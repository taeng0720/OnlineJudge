#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/juice@10.0.0/node_modules/juice/bin/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/juice@10.0.0/node_modules/juice/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/juice@10.0.0/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/juice@10.0.0/node_modules/juice/bin/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/juice@10.0.0/node_modules/juice/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/juice@10.0.0/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../juice@10.0.0/node_modules/juice/bin/juice" "$@"
else
  exec node  "$basedir/../../../../../juice@10.0.0/node_modules/juice/bin/juice" "$@"
fi
