#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/knip@5.17.4_@types+node@20.14.1_typescript@5.4.5/node_modules/knip/bin/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/knip@5.17.4_@types+node@20.14.1_typescript@5.4.5/node_modules/knip/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/knip@5.17.4_@types+node@20.14.1_typescript@5.4.5/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/knip@5.17.4_@types+node@20.14.1_typescript@5.4.5/node_modules/knip/bin/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/knip@5.17.4_@types+node@20.14.1_typescript@5.4.5/node_modules/knip/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/knip@5.17.4_@types+node@20.14.1_typescript@5.4.5/node_modules:/Users/taeng0720/Documents/GitHub/OnlineJudge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bun" ]; then
  exec "$basedir/bun"  "$basedir/../knip/bin/knip-bun.js" "$@"
else
  exec bun  "$basedir/../knip/bin/knip-bun.js" "$@"
fi
